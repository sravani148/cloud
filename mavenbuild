@Library ( 'Shared-Library')_

pipeline {
    agent any
parameters {
  string defaultValue: 'develop', name: 'BRANCH'
  string defaultValue: 'maven', name: 'BUILD'
}

    stages {
        stage('Checkout') {
            steps {
                script{
                  execgitoperations()
                   echo " ${params.BRANCH} " 
                  }
            }
        }
     stages {
        stage('Build') {
            steps {
                script{
                  build()
                  echo " ${params.BUILD} " 
                  }
            }
        }
    }
}
