@Library ( 'Shared-Library')_

pipeline {
    agent any
parameters {
  string defaultValue: 'develop', name: 'BRANCH'
  string defaultValue: 'maven', name: 'BUILD'
}

    stages {
        stage('Checkout') {
            steps {
                echo " ${params.BRANCH} " 
                script{
                  execgitoperations()                   
                  }
            }
        }
        stage('Build') {
            steps {
            echo " ${params.BUILD} " 
                script{
                  build()                  
                  }
            }
        }
    }
}
